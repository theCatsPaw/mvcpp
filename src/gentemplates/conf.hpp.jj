#include <string>
using namespace std;

#ifndef CONFIG_H_
#define CONFIG_H_


class Config {
private:
{% for key,val in conf.items() %}
	/**
	* @brief {{key}}
	*/
	string {{key.title()}} = "{{val}}";
{% endfor %}

public:
{% for key,val in conf.items() %}

	string get{{key.title()}}();

	string set{{key.title()}}(string new{{key.title()}});

{% endfor %}

};

#endif // CONFIG_H_